generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("MASTER_DATABASE_URL")
}

model Workspace {
  id            String   @id @default(cuid())
  name          String   @unique
  databaseUrl   String
  neonProjectId String?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt

  @@map("workspaces")
}